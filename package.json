{
	"name": "wikidata-misfit-bot",
	"private": true,
	"version": "2.1.7",
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-misfit-bot",
	"author": {
		"name": "EdJoPaTo",
		"email": "wikidata-misfit-bot@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"start": "node --loader ts-node/esm source/index.ts",
		"test": "tsc --sourceMap && xo && ava"
	},
	"type": "module",
	"engines": {
		"node": ">=14"
	},
	"dependencies": {
		"source-map-support": "^0.5.12",
		"telegraf": "^4.0.0",
		"telegraf-wikibase": "^0.10.1",
		"typegram": "^3.1.8",
		"wikidata-sdk-got": "^2.0.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^1.0.1",
		"@types/node": "^15.0.1",
		"ava": "^3.1.0",
		"del-cli": "^3.0.0",
		"ts-node": "^9.0.0",
		"typescript": "^4.2.3",
		"xo": "^0.40.1"
	},
	"ava": {
		"timeout": "70s"
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"@typescript-eslint/no-non-null-assertion": "off",
			"@typescript-eslint/prefer-readonly-parameter-types": "off",
			"ava/no-ignored-test-files": "off",
			"prefer-named-capture-group": "off",
			"unicorn/prefer-node-protocol": "off",
			"import/extensions": [
				"error",
				"ignorePackages"
			]
		}
	}
}
