{
	"name": "wikidata-misfit-bot",
	"private": true,
	"version": "3.0.1",
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-misfit-bot",
	"author": {
		"name": "EdJoPaTo",
		"email": "wikidata-misfit-bot@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"start": "node --loader ts-node/esm source/index.ts",
		"test": "tsc --sourceMap && xo && ava"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"dependencies": {
		"grammy": "^1.3.4",
		"source-map-support": "^0.5.12",
		"telegraf-wikibase": "^0.11.0",
		"wikidata-sdk-got": "^2.0.1"
	},
	"devDependencies": {
		"@grammyjs/types": "^2.2.6",
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/node": "^16.0.0",
		"ava": "^4.0.1",
		"del-cli": "^4.0.0",
		"ts-node": "^10.0.0",
		"typescript": "^4.2.3",
		"xo": "^0.47.0"
	},
	"ava": {
		"timeout": "70s"
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"@typescript-eslint/naming-convention": "off",
			"ava/no-ignored-test-files": "off",
			"node/prefer-global/process": "off",
			"prefer-named-capture-group": "off",
			"unicorn/prefer-node-protocol": "off",
			"import/extensions": [
				"error",
				"ignorePackages"
			]
		}
	}
}
